[${APP_NAME}]
Version: v${CMAKE_PROJECT_VERSION}
Description: ${APP_DESCRIPTION}
Author: ${APP_AUTHOR}
Platform: ${PLATFORM_NAME}

[Git]
Changes: ${GIT_CHANGES}
Branch: ${GIT_BRANCH}
Hash: ${GIT_HASH}
Date: ${GIT_DATE}

[Build]
Type: ${CMAKE_BUILD_TYPE}
System: ${CMAKE_SYSTEM_NAME}
Architecture: ${CMAKE_SYSTEM_PROCESSOR}

[Compiler]
Name: ${CMAKE_CXX_COMPILER_ID}
Version: ${CMAKE_CXX_COMPILER_VERSION}
C: ${CMAKE_C_COMPILER}
CPP: ${CMAKE_CXX_COMPILER}

[Host]
Name: ${CMAKE_HOST_SYSTEM_NAME}
Processor: ${CMAKE_HOST_SYSTEM_PROCESSOR}
Version: ${CMAKE_HOST_SYSTEM_VERSION}

[Runner]
${RUNNER_INFORMATION}
